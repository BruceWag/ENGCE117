#include <stdio.h>
#include <stdlib.h>

int *GetSet( int *n ) {
    int i;
    int *a;

    printf("Enter number of elements:");
    scanf("%d", n);

    a = (int *)malloc((*n) * sizeof(int));

    for(i = 0; i < *n; i++) {
        printf("Enter element %d: ", i + 1);
        scanf("%d", &a[i]);
    }

    return a;
}

int main() {
    int *data, num;

    data = GetSet( &num );

    return 0;
}
