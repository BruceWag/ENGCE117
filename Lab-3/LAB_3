#include <stdio.h>
#include <stdlib.h>

int *GetSet( int *n ) {
    int i;
    int *arr;

    printf("");
    scanf("%d", n);

    arr = (int *)malloc(*n * sizeof(int));

    for(i = 0; i < *n; i++) {
        printf("", i + 1);
        scanf("%d", &arr[i]);
    }

    return arr;
}

int main() {
    int *data, num;

    data = GetSet(&num);

    printf("");
    for(int i = 0; i < num; i++) {
        printf("%d ", data[i]);
    }

    free(data);
    return 0;
}
