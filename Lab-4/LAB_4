#include <stdio.h>
#include <stdlib.h>

int GetSet( int **arr ) {
    int n, i;

    printf("");
    scanf("%d", &n);

    *arr = (int *)malloc(n * sizeof(int));

    for(i = 0; i < n; i++) {
        printf("", i + 1);
        scanf("%d", &((*arr)[i]));
    }

    return n;
}

int main() {
    int *data, num;

    num = GetSet(&data);

    printf("");
    for(int i = 0; i < num; i++) {
        printf("%d ", data[i]);
    }

    free(data);
    return 0;
}
