#include <stdio.h>

void explode( char str1[], char splitter, char str2[][10], int *count ) {
    int i = 0;      
    int part = 0;   
    int ch = 0;     

    *count = 0;

    while (str1[i] != '\0') {
        if (str1[i] == splitter) {
            str2[part][ch] = '\0'; 
            part++;
            ch = 0;        
        } else {
            str2[part][ch] = str1[i];
            ch++;
        }
        i++;
    }

    str2[part][ch] = '\0';

    *count = part + 1;
}

int main() {
    char out[20][10];
    int num;

    explode("I/Love/You", '/', out, &num);

    printf("Count = %d\n", num);
    for (int i = 0; i < num; i++) {
        printf("str2[%d] = %s\n", i, out[i]);
    }

    return 0;
}
