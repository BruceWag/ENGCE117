#include <stdio.h>

void reverse( char str1[], char str2[] ) {
    int length = 0;
    int i;

    while (str1[length] != '\0') {
        length++;
    }

    for (i = 0; i < length; i++) {
        str2[i] = str1[length - 1 - i];
    }

    str2[length] = '\0';
}

int main() {
    char text[50] = "I Love You";
    char out[50];

    reverse(text, out);

    printf("%s\n", out);
    return 0;
}
