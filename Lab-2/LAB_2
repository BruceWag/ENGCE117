#include <stdio.h>
#include <stdlib.h>

void GetSet(int **arr, int *n) {
    int i;

    printf("");
    scanf("%d", n);

    *arr = (int *)malloc(*n * sizeof(int));

    for (i = 0; i < *n; i++) {
        printf("", i + 1);
        scanf("%d", &((*arr)[i]));
    }
}

int main() {
    int *data, num;

    GetSet(&data, &num);

    printf("");
    for (int i = 0; i < num; i++) {
        printf("%d ", data[i]);
    }

    free(data);
    return 0;
}
